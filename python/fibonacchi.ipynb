   "source": [
    "def fibonacci(n, init=1):\n",
    "    array =[init]\n",
    "    for i in range(n-1):\n",
    "        if i==0:\n",
    "            array.append(array[i])\n",
    "            continue\n",
    "        array.append(array[i]+array[i-1])\n",
    "    return array"
   ]
